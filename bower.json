{
  "name": "php7-presentation",
  "description": "PHP7 Presentation",
  "license": "LGPL-3.0",
  "main": "gulpfile.js",
  "authors": [
    "crazy-max"
  ],
  "private": false,
  "dependencies": {
    "reveal.js": "^3.4.1",
    "google-fonts-montserrat": "https://fonts.googleapis.com/css?family=Montserrat:400,700",
    "google-fonts-rajdhani": "https://fonts.googleapis.com/css?family=Rajdhani:400,700,600,500,300",
    "zepto": "^1.2.0",
    "font-awesome": "^4.7.0"
  },
  "overrides": {
    "reveal.js": {
      "main": [
        "js/*.js",
        "css/*.css",
        "css/theme/black.css",
        "lib/css/zenburn.css",
        "lib/font/**",
        "lib/js/classList.js",
        "lib/js/html5shiv.js",
        "plugin/highlight/**",
        "plugin/markdown/**",
        "plugin/notes/**",
        "plugin/print-pdf/**",
        "plugin/zoom-js/**"
      ]
    },
    "font-awesome": {
      "main": [
        "css/font-awesome.css",
        "fonts/*.*"
      ]
    },
    "google-fonts-montserrat": {
      "main": "index"
    },
    "google-fonts-rajdhani": {
      "main": "index"
    }
  },
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ]
}
